# This file is preprocessed during the qmake pass
# The pre-processing is done in the root project .pro file
# see: define_appname.pri
LOWERED_APPNAME=$${LOW_APPNAME}
DEFINES*=$${UP_APPNAME}
$${UP_APPNAME}=1
BUILD_PATH_POSTFIXE = $${CAMELCASE_APPNAME}

# manages extra plugins
with-agenda { DEFINES += WITH_AGENDA }
with-pad    { DEFINES += WITH_PAD }
with-alerts { DEFINES += WITH_ALERTS }
with-feedback { DEFINES += WITH_FEEDBACK }
with-webcam { DEFINES += WITH_WEBCAM }
